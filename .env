# Private key for the deployer wallet (0x-prefixed, without quotes). KEEP SECRET.
DEPLOYER_PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# RPC endpoints
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Token parameters
TOKEN_NAME=Randy
TOKEN_SYMBOL=RANDY
TOKEN_DECIMALS=18
# Whole token amount (will be converted to wei using TOKEN_DECIMALS)
TOKEN_SUPPLY=1000000000
# Optional: explicit owner address (0x...). If empty, defaults to first signer.
TOKEN_OWNER=

# Optional: where your token logo is hosted (HTTPS).
TOKEN_LOGO_URI=https://YOUR_DOMAIN/steermeme-logo.png
# Chain to record in tokenlist updates (84532 for Base Sepolia, 8453 for Base)
CHAIN_ID=84532
